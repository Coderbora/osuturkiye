{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "./build/server",
		"rootDir": "./src/server",
		"sourceMap": true,
		"target": "es2015",
		"module": "commonjs",
		"lib": [
			"es2017"
		],
		"skipLibCheck": true
	},
	"files": [
		"src/server/index.ts"
	],
	"include": [
		"src/server/scripts/*.ts",
		"src/server/discord/commands/*.ts"
	]
}
